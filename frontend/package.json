    "jest-dom": "^4.0.0",
    "jest-styled-components": "^7.0.3",
    "lint-staged": ">=10",
    "react-testing-library": "^8.0.1",
    "typescript": "^4.0.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{js,ts,tsx,md,yaml,yml,graphql}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
