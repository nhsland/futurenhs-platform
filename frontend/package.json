{
  "name": "frontend",
  "license": "MIT",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn generate",
    "dev": "NODE_ENV=development node server.js",
    "dev:trace": "NODE_ENV=development NODE_OPTIONS='--require ./tracing.js' node server.js",
    "prebuild": "yarn generate",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production NODE_OPTIONS='--require ./tracing.js' node server.js",
    "pretest": "yarn generate",
    "test": "jest",
    "lint": "yarn eslint . --ext .js,.ts,.tsx",
    "clean": "rm -rf .next",
    "generate": "graphql-codegen --config codegen.yaml"
  },
  "dependencies": {
    "@apollo/federation": "^0.20.0",
    "@apollo/gateway": "^0.20.1",
    "@azure/monitor-opentelemetry-exporter": "^1.0.0-preview.5",
    "@fnhs/event-models": "file:../event-models/typescript/",
    "@microsoft/applicationinsights-web": "^2.5.8",
    "@opentelemetry/node": "^0.11.0",
    "@opentelemetry/plugin-https": "^0.11.0",
    "@opentelemetry/tracing": "^0.11.0",
    "@types/testing-library__react": "^10.2.0",
    "apollo-server-micro": "^2.17.0",
    "axios": "^0.20.0",
    "azure-eventgrid": "^1.6.0",
    "classnames": "^2.2.6",
    "connect-pg-simple": "^6.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "graphql-request": "^3.1.0",
    "isomorphic-fetch": "^2.2.1",
    "ms-rest-azure": "^3.0.0",
    "next": "9.5.3",
    "next-images": "^1.4.1",
    "nhsuk-frontend": "^3.1.0",
    "nhsuk-react-components": "^1.0.0-rc.4",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
    "passport-azure-ad": "^4.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^6.8.2",
    "sass": "^1.26.11",
    "sass-extract": "^2.1.0",
    "sass-extract-js": "^0.4.0",
    "sass-extract-loader": "^1.1.0",
    "styled-components": "^5.2.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-typescript": "^7.10.4",
    "@graphql-codegen/cli": "1.17.8",
    "@graphql-codegen/typescript": "1.17.9",
    "@graphql-codegen/typescript-graphql-request": "^2.0.0",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@testing-library/react": "^11.0.4",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.49",
    "@types/styled-components": "^5.1.3",
    "@types/testing-library__jest-dom": "^5.9.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-jest": "^26.3.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.9.0",
    "eslint-plugin-import": "^2.22.0",
